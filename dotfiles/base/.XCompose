include "%L"

<Multi_key> <asciitilde> <asciitilde> : "≈" U2248 # ALMOST EQUAL TO
<Multi_key> <equal> <greater>         : "⇒" U21D2 # RIGHTWARDS DOUBLE ARROW
<Multi_key> <comma> <period>          : "⟷" U27F7 # LONG LEFT RIGHT ARROW
<Multi_key> <period> <comma>          : "⟷" U27F7 # LONG LEFT RIGHT ARROW

<Multi_key> <slash> <question>        : "⸮" U2E2E # REVERSED QUESTION MARK
<Multi_key> <question> <slash>        : "⸮" U2E2E # REVERSED QUESTION MARK

<Multi_key> <p> <i>                   : "π" U03C0 # GREEK SMALL LETTER PI
<Multi_key> <P> <I>                   : "Π" U03A0 # GREEK CAPITAL LETTER PI
<Multi_key> <p> <h> <i>               : "φ" U03A6 # GREEK SMALL LETTER PHI
<Multi_key> <P> <H> <I>               : "Φ" U03C6 # GREEK CAPITAL LETTER PHI
<Multi_key> <p> <s> <i>               : "ψ" U03C8 # GREEK SMALL LETTER PSI
<Multi_key> <P> <S> <I>               : "Ψ" U03A8 # GREEK CAPITAL LETTER PSI
<Multi_key> <s> <i> <g>               : "σ" U03C3 # GREEK SMALL LETTER SIGMA
<Multi_key> <S> <I> <G>               : "Σ" U03A3 # GREEK CAPITAL LETTER SIGMA

<Multi_key> <a> <l> <l>               : "∀" U2200 # FOR ALL
<Multi_key> <e> <x>                   : "∃" U2203 # THERE EXISTS
<Multi_key> <s> <o>                   : "∴" U2234 # THEREFORE
<Multi_key> <b> <e> <c>               : "∵" U2235 # BECAUSE

<Multi_key> <e> <u> <r>               : "€"   EuroSign # EURO SIGN

<Multi_key> <o> <h> <m>               : "Ω" U2126 # OHM SIGN

<Multi_key> <l> <o> <d>               : "ಠ_ಠ"         # emoji: ‘look of disapproval’
<Multi_key> <t> <a> <b> <l> <e>       : "(╯°□°)╯︵ ┻━┻" # emoji: ‘flipping table’
<Multi_key> <s> <h> <r> <u> <g>       : "¯\\_(ツ)_/¯"   # emoji: ‘shrug’
<Multi_key> <h> <a> <p> <p> <y>       : "(ﾉ^_^)ﾉ"      # emoji: ‘squinty happy arms up’
